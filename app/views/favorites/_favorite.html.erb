  <% if music.favorited_by?(current_user) %>
    <td><%= link_to music.favorites.count, music_favorites_path(music), method: :delete, class: "fas fa-heart heart_red text-danger" %></td>
  <% else %>
    <td><%= link_to music.favorites.count, music_favorites_path(music), method: :post,  class: "fas fa-heart heart_blue" %></td>
  <% end %>